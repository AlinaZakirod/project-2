<h1>Create New Layout</h1>
<form action="/layout/new" method="POST">
<div>
  <label for="title">Title:</label>
  <input type="text" name="title" id="title">
  <label for="objects">Pieces of Furniture</label>
  <select  multiple type ="text"  name="furnitureObjects">
    {{#each allFurniture}}
      <option value="{{this._id}}">{{this.title}}</option>
    {{/each}}
  </select>
</div>
<button type="submit">Submit</button>
</form>



<div class="accordion" id="accordionExample">
